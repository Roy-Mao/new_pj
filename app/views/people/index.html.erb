<p style="color: green"><%= notice %></p>

<h1>People</h1>


<div id="people">
  
  <% @people.each do |person| %>
    <div>
    <%= person.id %>
    <%= person.name %>
    <%= person.phone_number %>
    <%= person.email %>
    </div>
  <% end %>
</div>

<%= link_to "New person", new_person_path %>
