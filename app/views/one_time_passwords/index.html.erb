<p style="color: green"><%= notice %></p>

<h1>One time passwords</h1>

<div id="one_time_passwords">
  <% @one_time_passwords.each do |one_time_password| %>
    <%= render one_time_password %>
    <p>
      <%= link_to "Show this one time password", one_time_password %>
    </p>
  <% end %>
</div>

<%= link_to "New one time password", new_one_time_password_path %>
